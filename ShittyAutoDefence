local Section3 = Tabb:NewSection("Spam codes")
Section3:NewButton("Spam mochi code", "Mochi", function()
    local code = "MOCHITOMORROW"

for i = 1, 500 do
    task.spawn(function()
        game:GetService("Players").LocalPlayer.PlayerGui.MainGui.TwitterCodesFrame.Code.LocalScript.RemoteFunction:InvokeServer(code)
    end)
end

end)
Section3:NewButton("Spam bug code", "Mochi", function()
    local code = "SORRY4BUG"

for i = 1, 500 do
    task.spawn(function()
        game:GetService("Players").LocalPlayer.PlayerGui.MainGui.TwitterCodesFrame.Code.LocalScript.RemoteFunction:InvokeServer(code)
    end)
end
end)

local Section4 = Tabb:NewSection("Auto stuff")
Section4:NewButton("Auto Armament Haki", "Nigga haki", function()

    game.Players.LocalPlayer.CharacterAdded:connect(function()
wait(5.5)
local args = {
    [1] = "Fighting Style",
    [2] = "G",
    [3] = CFrame.new(-577.0409545898438, -3.3241958618164062, -4107.40234375) * CFrame.Angles(-1.302381157875061, 1.2543939352035522, 1.2890747785568237),
    [4] = workspace.Map.Islands.Marineford.Model.Marineford:FindFirstChild("Marineford platform"):FindFirstChild("Marineford platform").MeshPart,
    [5] = 5
}

game:GetService("ReplicatedStorage").Remotes.requestAbility:FireServer(unpack(args))
end)
end)

Section4:NewToggle("Auto Observation Haki", "Observation Haki", function(bool)
    if bool then
        getgenv().ob = true
        while getgenv().ob == true do
            wait()
local args = {
    [1] = "Fighting Style",
    [2] = "T",
    [3] = CFrame.new(-577.0409545898438, -3.3241958618164062, -4107.40234375) * CFrame.Angles(-1.302381157875061, 1.2543939352035522, 1.2890747785568237),
    [4] = workspace.Map.Islands.Marineford.Model.Marineford:FindFirstChild("Marineford platform"):FindFirstChild("Marineford platform").MeshPart,
    [5] = 5
}

game:GetService("ReplicatedStorage").Remotes.requestAbility:FireServer(unpack(args))
end
    else
    getgenv().ob = false
    end
end)
Section4:NewToggle("Auto drown", "drown", function(bool)
    if bool then
        getgenv().drown = true
        while getgenv().drown == true do
            wait()
game.Players.LocalPlayer.Character.Humanoid:Destroy()

game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(Character)
    wait(4.1)
    while getgenv().drown == true do
        wait()
    local plr = game:service"Players".LocalPlayer;
local tween_s = game:service"TweenService";
local info = TweenInfo.new(0.5,Enum.EasingStyle.Quad);
function tp(...)
   local tic_k = tick();
   local params = {...};
   local cframe = CFrame.new(params[1],params[2],params[3]);
   local tween,err = pcall(function()
       local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
       tween:Play();
   end)
   if not tween then return err end
end
tp(248, -100, -206, 0, -8, 1, -9, 1, 1, -1);
end

end)
end
    else
    getgenv().drown = false
    end
end)
Section4:NewTextBox("Auto delete put item name", "Item", function(item)
    getgenv().delete = true
    while getgenv().delete == true do
        wait()
     if game.Players.LocalPlayer.Inventory[item].Value > 1 then
wait(5)
game:GetService("ReplicatedStorage").Remotes.drop:FireServer( game:GetService("Players").LocalPlayer.Character:FindFirstChild(item))
end
end
end)
Section4:NewButton("Stop autodelete", "stop", function()
    getgenv().delete = false
end)
local Section3 = Tabb:NewSection("Devil Fruits")
Section3:NewButton("Buy random devil", "buy", function()
  game:GetService("ReplicatedStorage").Remotes.buyrngdf:FireServer()
end)  
Section3:NewToggle("Auto buy fruit", "buy", function(bool)
if bool then
    getgenv().buy =true
    while getgenv().buy == true do
        wait(5)
      

game:GetService("ReplicatedStorage").Remotes.buyrngdf:FireServer()

    end
    else
        getgenv().buy = false
        end
end)
Section3:NewButton("Stop buying devil fruits", "stop", function()
    getgenv().buy = false
end)
local Section = Tab:NewSection("Main")
Section:NewToggle("Autofarm Defense", "Autofarms Defense", function(state)
    if state then
        canLoop = true
        while wait() and canLoop do
    if game.Players.LocalPlayer.CharacterAdded then
    local plr = game:service"Players".LocalPlayer;
local tween_s = game:service"TweenService";
local info = TweenInfo.new(0.5,Enum.EasingStyle.Quad);
function tp(...)
   local tic_k = tick();
   local params = {...};
   local cframe = CFrame.new(params[1],params[2],params[3]);
   local tween,err = pcall(function()
       local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
       tween:Play();
   end)
   if not tween then return err end
end
tp(-603, 0, -4338, 0, -6, -0, 6, 1, 2);

end
end

    else
        canLoop = false
    end
end)
local Section2 = Tab:NewSection("Auto skills")
